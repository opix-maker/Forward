[{"id":48761,"p":"/gIZ3IY6Ea0DkvhtI8TqXsHoyjkN.jpg","b":"/6hhzasM2fl3xpUVm8K8y6M7b2Kk.jpg","t":"Klatretøsen","r":5.646,"y":2002,"rd":"2002-01-25","hs":0.135,"d":0.25,"mt":"movie","o":""},{"id":83770,"p":"/d77xMAuWpCy8nEkWJaOpH30VXbK.jpg","b":"/VMDxK7avGmST3UPReaxFum4Szg.jpg","t":"在路上","r":5.614,"y":2012,"rd":"2012-05-22","hs":0.812,"d":2.098,"mt":"movie","o":"改编自同名原著小说，小说创作于1957年，表现了战后美国青年的精神空虚和浑浑噩噩的状态，被视为“垮掉的一代”奠基之作，主要写“我”同一个狂热的垮掉分子迪安·莫里亚蒂以及其他几个朋友三次从美国西海岸到东海岸的穿梭旅行。在自己的父亲去世之后，处在天人交战、心情不佳时期的作家萨尔·派瑞戴斯决定要开始一趟横跨美国的旅途，去寻找“它”的意义。和他一起上路的是他的好朋友迪恩·莫里亚蒂。迪恩是一个天生的旅行家和神秘主义者。这一路上，发生了各种各样的怪事，而这两个年轻人也渐渐找到了自己生命的方向。"},{"id":1441809,"p":"/27eyCVo8evQEFRfE3gzGt6cIEu0.jpg","b":null,"t":"The Stand","r":0,"y":1994,"rd":"1994-05-08","hs":0.222,"d":0.521,"mt":"movie","o":""},{"id":1495232,"p":"/3dlfznykY3NNyLE6qZrcD1QTsyf.jpg","b":"/n4TQ8uqOFJ3x8jA7srM2jA4U2OO.jpg","t":"Merlin","r":0,"y":1998,"rd":"1998-04-26","hs":0.173,"d":0.36,"mt":"movie","o":""}]