[{"id":40961,"p":"/vtwQ8oICMxscIpoL3bKcoKklcV2.jpg","b":null,"t":"魔鬼赛门","r":6,"y":1997,"rd":"1997-08-26","hs":0.186,"d":0.401,"mt":"movie","o":"赛门是一位超级恐怖杀手，在精神病院住了五年之久。该精神病院由凯伦医生所负责，她想证明赛门已完全痊愈，对外界没有任何威协，于是她决定亲自与赛门做最后一次谈话，以确定他是否可以离开精神病院。一开始，凯伦对赛门抱持着相信的态度。但是不久后，随着赛门的言语，他的恶魔真面目逐渐浮现，他怪异的内心世界令人不寒而栗。这不但混淆了凯伦对现实的认知，甚至威协到她的生命！"},{"id":454826,"p":"/3gAHLH1E2OqJllvT5VP1k7p4tM2.jpg","b":"/oRup1f5jFCr76GiBjedYytpzkrM.jpg","t":"villain","r":6,"y":2017,"rd":"2017-10-27","hs":0.306,"d":0.376,"mt":"movie","o":"谁挑逗整个警察部门与他完美无瑕的犯罪杀手，获得由退休的时尚警察，他过去与猫捉老鼠的游戏得到手了影响杀手的未来追赶。"},{"id":26791,"p":"/8CM2YIX0qLcqDY7d6iVb4uRXGG8.jpg","b":"/ag90STh8Gvinuwg8Q0NRAcY3JAm.jpg","t":"Brigham City","r":6.5,"y":2001,"rd":"2001-03-30","hs":0.169,"d":0.328,"mt":"movie","o":""},{"id":97686,"p":"/bLSI26TOpRx0tMyjlRQzTO2aUWF.jpg","b":"/yvpKHS9JLaDvPVuMpGyhqtZ9tH8.jpg","t":"The Jeffrey Dahmer Files","r":5.6,"y":2013,"rd":"2013-02-15","hs":0.191,"d":0.267,"mt":"movie","o":""},{"id":289575,"p":"/u5c5SxGfEu5cgxErjCa0EXISKL4.jpg","b":null,"t":"残酷睡客的故事","r":6.4,"y":2014,"rd":"2014-08-29","hs":0.183,"d":0.244,"mt":"movie","o":""}]